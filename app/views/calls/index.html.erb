<h1>Listing calls</h1>

<table>
  <tr>
    <th>Constituent</th>
    <th>Campaign</th>
    <th>Call date</th>
    <th>Call time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calls.each do |call| %>
  <tr>
    <td><%= call.constituent_id %></td>
    <td><%= call.campaign_id %></td>
    <td><%= call.call_date %></td>
    <td><%= call.call_time %></td>
    <td><%= link_to 'Show', call %></td>
    <td><%= link_to 'Edit', edit_call_path(call) %></td>
    <td><%= link_to 'Destroy', call, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Call', new_call_path %>
